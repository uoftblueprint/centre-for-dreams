{
  "name": "@cfd/expo",
  "version": "0.1.0",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev": "pnpm with-env expo start --go",
    "dev:tunnel": "pnpm with-env expo start --go --tunnel",
    "dev:android": "pnpm with-env expo start --android",
    "dev:ios": "pnpm with-env expo start --ios",
    "lint": "eslint .",
    "lint:expo": "expo-doctor",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit",
    "android": "pnpm with-env expo run:android",
    "ios": "pnpm with-env expo run:ios",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^0.20.5",
    "@expo-google-fonts/poppins": "^0.2.3",
    "@expo/metro-config": "^0.10.7",
    "@react-native-firebase/app": "^19.0.0",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.1",
    "@trpc/react-query": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "date-fns": "^2.30.0",
    "expo": "^49.0.22",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.13",
    "expo-device": "~5.4.0",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-notifications": "~0.20.1",
    "expo-router": "2.0.14",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-root-toast": "^3.5.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.1",
    "react-native-svg": "13.9.0",
    "react-native-svg-transformer": "^1.3.0",
    "superjson": "2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@cfd/api": "^0.1.0",
    "@cfd/eslint-config": "^0.2.0",
    "@cfd/prettier-config": "^0.1.0",
    "@cfd/tailwind-config": "^0.1.0",
    "@cfd/tsconfig": "^0.1.0",
    "@expo/config-plugins": "^7.2.5",
    "@types/babel__core": "^7.20.5",
    "@types/react": "^18.2.59",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "expo-doctor": "^1.3.0",
    "prettier": "^3.2.5",
    "tailwindcss": "3.3.2",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@cfd/eslint-config/base",
      "@cfd/eslint-config/react"
    ]
  },
  "prettier": "@cfd/prettier-config"
}
