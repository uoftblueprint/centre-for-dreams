{
  "name": "@cfd/expo",
  "version": "0.1.0",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev": "pnpm with-env expo start --go",
    "dev:tunnel": "pnpm with-env expo start --go --tunnel",
    "dev:android": "pnpm with-env expo start --android",
    "dev:ios": "pnpm with-env expo start --ios",
    "lint": "eslint .",
    "lint:expo": "expo-doctor",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit",
    "android": "pnpm with-env expo run:android",
    "ios": "pnpm with-env expo run:ios",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^0.20.35",
    "@expo-google-fonts/poppins": "^0.2.3",
    "@expo/metro-config": "^0.19.11",
    "@expo/prebuild-config": "~8.0.28",
    "@supabase/supabase-js": "^2.39.7",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "date-fns": "^2.30.0",
    "expo": "^52.0.39",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.14",
    "expo-device": "~7.0.2",
    "expo-font": "~13.0.3",
    "expo-image-picker": "^16.0.4",
    "expo-linking": "~7.0.4",
    "expo-notifications": "~0.29.14",
    "expo-router": "4.0.19",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.21",
    "expo-status-bar": "~2.0.1",
    "metro": "^0.81.0",
    "metro-config": "^0.81.0",
    "metro-resolver": "^0.81.0",
    "nativewind": "^2.0.11",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-root-toast": "^3.5.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-svg-transformer": "^1.3.0",
    "superjson": "2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/runtime": "^7.27.1",
    "@cfd/api": "~0.1.0",
    "@cfd/eslint-config": "^0.2.0",
    "@cfd/prettier-config": "^0.1.0",
    "@cfd/tailwind-config": "^0.1.0",
    "@cfd/tsconfig": "^0.1.0",
    "@expo/config-plugins": "^9.0.16",
    "@types/babel__core": "^7.20.5",
    "@types/react": "^18.2.79",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.57.1",
    "expo-doctor": "^1.3.0",
    "prettier": "^3.4.2",
    "tailwindcss": "3.3.2",
    "typescript": "^5.7.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@cfd/eslint-config/base",
      "@cfd/eslint-config/react"
    ]
  },
  "prettier": "@cfd/prettier-config",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
