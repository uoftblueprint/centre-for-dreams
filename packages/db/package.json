{
  "name": "@cfd/db",
  "version": "0.1.0",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "db:generate": "pnpm with-env prisma generate",
    "db:push": "pnpm with-env prisma db push --skip-generate",
    "db:migrate": "pnpm with-env prisma migrate dev",
    "db:migrate:prod": "pnpm with-env prisma migrate deploy",
    "dev": "pnpm with-env prisma studio --port 5556",
    "format": "prettier --check \"**/*.{mjs,ts,md,json,prisma}\"",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@cfd/logger": "^0.1.0",
    "@neondatabase/serverless": "^0.6.0",
    "@prisma/adapter-neon": "^5.6.0",
    "@prisma/client": "^5.6.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@cfd/eslint-config": "^0.2.0",
    "@cfd/prettier-config": "^0.1.0",
    "@cfd/tsconfig": "^0.1.0",
    "@types/ws": "^8.5.10",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "typescript": "^5.3.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@cfd/eslint-config/base"
    ]
  },
  "prettier": "@cfd/prettier-config",
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.3"
  }
}
