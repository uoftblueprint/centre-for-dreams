{
  "name": "@cfd/expo",
  "version": "0.1.0",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev": "pnpm with-env expo start --go",
    "dev:tunnel": "pnpm with-env expo start --go --tunnel",
    "dev:android": "pnpm with-env expo start --android",
    "dev:ios": "pnpm with-env expo start --ios",
    "lint": "eslint .",
    "lint:expo": "expo-doctor",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit",
    "android": "pnpm with-env expo run:android",
    "ios": "pnpm with-env expo run:ios",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^0.20.10",
    "@expo-google-fonts/poppins": "^0.2.3",
    "@expo/metro-config": "^0.18.11",
    "@supabase/supabase-js": "^2.39.7",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "date-fns": "^2.30.0",
    "expo": "^51.0.35",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.27",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.10",
    "expo-image-picker": "^15.0.7",
    "expo-linking": "~6.3.1",
    "expo-notifications": "~0.28.18",
    "expo-router": "3.5.23",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.6",
    "expo-status-bar": "~1.12.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-root-toast": "^3.5.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-svg-transformer": "^1.3.0",
    "superjson": "2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/runtime": "^7.25.6",
    "@cfd/api": "^0.1.0",
    "@cfd/eslint-config": "^0.2.0",
    "@cfd/prettier-config": "^0.1.0",
    "@cfd/tailwind-config": "^0.1.0",
    "@cfd/tsconfig": "^0.1.0",
    "@expo/config-plugins": "^8.0.10",
    "@types/babel__core": "^7.20.5",
    "@types/react": "^18.2.79",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "expo-doctor": "^1.3.0",
    "prettier": "^3.2.5",
    "tailwindcss": "3.3.2",
    "typescript": "^5.4.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@cfd/eslint-config/base",
      "@cfd/eslint-config/react"
    ]
